     [Quantidade Licenciada], 
     Cilindradas, 
     Potência, 
     [Código de Carroceria], 
     [Número de Lugares], 
     [Tipo de Comprador], 
     [Tipo de Veículo], 
     [Renavam / Potência], 
     Produto, 
     [Tipo Produto], 
     Marca, 
     [Marca / Modelo], 
     Classe, 
     [Sub-Classe], 
     [Indústria Servida], 
     MapSubString('Letras',Upper (UF & Município)) as %Chave_Mapa,
     Município, 
     UF, 
     [Grupo Econômico], 
     Concessionária, 
     [Grupo Econômico 2], 
     [Soma Classes], 
     [Mercado Total], 
     [Grupo Econômico 3], 
     [Tipo de Carroceria], 
     Linha, 
